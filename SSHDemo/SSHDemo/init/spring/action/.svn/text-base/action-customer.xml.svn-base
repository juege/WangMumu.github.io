<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd"
	default-autowire="byName">
	
	<bean id="customerManagerDto" class="com.chinarewards.framework.user.dto.CustomerManagerDto" scope="prototype"></bean>
	
	<bean id="customerAction" class="com.chinarewards.framework.user.action.CustomerAction" scope="prototype">
		<property name="customerManagerDto" ref="customerManagerDto"></property>
	</bean>

</beans>

