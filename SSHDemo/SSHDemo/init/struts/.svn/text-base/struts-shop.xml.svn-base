<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="shop" extends="struts-default">
		<action name="findShop" class="shopAction" method="findShopAction">
			<result name="shop">WEB-INF/pages/shop.jsp</result>
			<result name="shopNull">WEB-INF/pages/tbShopNull.jsp</result>
		</action>
		
		<action name="convertShop" class="com.chinarewards.framework.taobao.shop.action.convertShopAction" method="convertShop">
			<result name="success">WEB-INF/pages/shopsDetail.jsp</result>
		</action>
	</package>
</struts>