<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
     <!-- 登录相关 -->
     <package name="login"  extends="json-default">
     
        <action name="saveID" class="loginAction" method="saveID">
            <result name="success" type="json">

                <!-- 这里指定将被Struts2序列化的属性，该属性在action中必须有对应的getter方法 -->

                <param name="root">message</param>

            </result>

            
        </action>
        <action name="syncSession" class="loginAction" method="syncSession">
            <result name="success" type="json">

                <!-- 这里指定将被Struts2序列化的属性，该属性在action中必须有对应的getter方法 -->

                <param name="root">message</param>
<!--                 <param name="callbackParameter">jsoncallback</param> -->
            </result>
        </action>
        
        <action name="syncTBData" class="loginAction" method="syncTBData">
            <result name="success" type="json">

                <!-- 这里指定将被Struts2序列化的属性，该属性在action中必须有对应的getter方法 -->

                <param name="root">message</param>
               
            </result>
        </action>
       
        <action name="findTB" class="loginAction" method="findTB">
   
<!--          	    <result name="success" type="json">

                这里指定将被Struts2序列化的属性，该属性在action中必须有对应的getter方法

                <param name="root">taobao</param>
                <param name="callbackParameter">jsoncallback</param>
            </result>
-->
        	<result name="success">WEB-INF/pages/tbOpenUrl.jsp</result>
        </action>
        
        <action name="quitUrl" class="loginAction" method="quitUrl">
        	<result name="success" type="redirectAction">/findHomeItems.action</result>
        </action>
        
        <action name="loginPage" class="loginAction" method="loginPage">
        	<result name="success">WEB-INF/pages/loginPage.jsp</result>
        </action>
      </package>
</struts>